{
  "extends": "../../tslint.json",
  "linterOptions": {
    "exclude": ["build", "coverage", "node_modules"]
  },
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "ugl",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "ugl",
      "kebab-case"
    ]
  }
}
